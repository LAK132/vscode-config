{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "windows",
      "request": "attach",
      "type": "cppvsdbg",
      "processId": "${command:pickProcess}",
    },
    {
      "name": "linux",
      "program": "${workspaceRoot}/bin/output.elf",
      "request": "launch",
      "type": "cppdbg",
      "cwd": "${workspaceRoot}/bin",
    }
  ]
}
